<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sathya.linearlayoutexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LinearLayoutExample">
        <activity android:name=".IntentActivity"></activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".AlertUpdatedActivity" />
        <activity android:name=".AlertActivity" />
        <activity android:name=".DynamicActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MaiActivity" />
        <activity android:name=".ButtonActivity" />
        <activity android:name=".StaticListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>